+-        workManager.enqueueUniqueWork(
+-            exactWorkName,
+-            androidx.work.ExistingWorkPolicy.KEEP,
+-            exactWorkRequest
+-        )
+-
+-        Log.d(TAG, "'EXACT TIME' work enqueued successfully")
+-        Log.d(TAG, "--- scheduleOneTimeReminder END ---")
+-    }
