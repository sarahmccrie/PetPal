+-                        }
+-                    },
+-                    onAddAppointment = { appointment ->
+-                        coroutineScope.launch {
+-                            reminderViewModel.addAppointment(appointment)
+-                        }
+-                    }
+-                )
+-            }
+-        }
+-    }
